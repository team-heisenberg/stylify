{
  "expo": {
    "name": "Stylify",
    "slug": "stylify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "ca.stylify.app",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/logo/LogoAnimation.gif",
      "resizeMode": "contain",
      "backgroundColor": "#F9F5EE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ca.stylify.app",
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ebd22600-10f7-406c-ac2d-7c9957b96831"
      }
    },
    "owner": "zyzmoz"
  }
}
