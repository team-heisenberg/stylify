{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "dotenv -e ../.env -- tsnd --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "dev": "dotenv -e ../.env -- ENVIRONMENT=development tsnd --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "build": "dotenv -e ../.env prisma generate && rimraf -rf ./build && tsc",
    "start:docker": "dotenv -e ../.env -- docker compose -f ../.docker/docker-compose.yml -p stylify up  --detach --no-deps --build",
    "start:db": "dotenv -e ../.env -- docker compose -f ../.docker/docker-mysql.yml -p stylify up --detach --no-deps --build",
    "start:redis": "dotenv -e ../.env -- docker compose -f ../.docker/docker-redis.yml -p stylify up --detach --no-deps --build",
    "start:db:m1": "dotenv -e ../.env -- docker compose -f ../.docker/docker-mysql-apple-silicon.yml -p stylify up --detach --no-deps --build",
    "prisma:migrate": "dotenv -e ../.env npx prisma migrate dev --name",
    "prisma:deploy": "dotenv -e ../.env npx prisma migrate deploy",
    "prisma:resolve": "dotenv -e ../.env npx prisma migrate resolve",
    "prisma:seed": "dotenv -e ../.env ts-node seeds/seed.ts",
    "prisma:seed:prod": "dotenv -e ../.env ts-node seeds/seed-prod.ts",
    "prisma:reset": "dotenv -e ../.env npx prisma migrate reset",
    "prisma:generate": "dotenv -e ../.env npx prisma generate",
    "test": "dotenv -e ../.env -- jest",
    "lint": "npx eslint . --ext .js,.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@types/ioredis": "5.0.0",
    "app": "^0.1.0",
    "axios": "^1.3.3",
    "bcrypt": "^5.1.0",
    "cd": "^0.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ioredis": "5.3.1",
    "jsonwebtoken": "^9.0.0",
    "prisma": "^4.9.0",
    "prisma-redis-middleware": "4.6.1"
  }
}
